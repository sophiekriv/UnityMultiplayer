        function setDetailAttributes(detailId, attr) {
            var names = {
                ci: 'cacheId',
                ts: 'thumbnailsSmall',
                tl: 'thumbnailsLarge',
                tz: 'thumbnailsZoom',
                mk: 'mainKeywords',
                mkl: 'mainKeywordsLabel',
